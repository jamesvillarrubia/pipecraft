{
  "name": "logging",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/logging",
        "main": "libs/logging/src/index.ts",
        "tsConfig": "libs/logging/tsconfig.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/logging/jest.config.ts"
      }
    },
    "integration-test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/logging/jest.config.ts"
      }
    }
  }
}
