{
  "name": "database",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/database",
        "main": "libs/database/src/index.ts",
        "tsConfig": "libs/database/tsconfig.json"
      }
    },
    "test": {
      "dependsOn": ["build"],
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/database/jest.config.ts"
      }
    },
    "integration-test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/database/jest.config.ts"
      }
    }
  }
}
